{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*", "src/*"]
    }
  },
  "include": [
    "src/ui/**/*.ts", 
    "src/support/**/*.ts"
  ],
  "exclude": [
    "src/api/**/*.ts",
    "src/support/hooks/api.hooks.ts",
    "generatedModels/**/*.ts",
    "node_modules"
  ]
}